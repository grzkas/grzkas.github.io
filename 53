#include <iostream>
using namespace std;

int main()
{
    const int N = 7;
    int tab_kw[N];

    
    cout << "Podaj numer dziennika: ";
    cin >> tab_kw[0];

    cout << "Podaj liczbe liter imienia: ";
    cin >> tab_kw[1];

    cout << "Podaj swoj wzrost (cm): ";
    cin >> tab_kw[2];

    cout << "Podaj liczbe liter nazwiska: ";
    cin >> tab_kw[3];

    cout << "Podaj ostatnie dwie cyfry roku: ";
    cin >> tab_kw[4];

    cout << "Podaj dzisiejszy dzien: ";
    cin >> tab_kw[5];

    cout << "Podaj obecny miesiac: ";
    cin >> tab_kw[6];

    
    cout << "\nTablica przed sortowaniem:\n";
    for(int i = 0; i < N; i++)
        cout << tab_kw[i] << " ";
    cout << endl;

    
    int Bufor_trzy_pierwsze_litery;

    for(int i = 0; i < N - 1; i++)
    {
        int min = i;

        for(int j = i + 1; j < N; j++)
        {
            if(tab_kw[j] < tab_kw[min])
                min = j;
        
        Bufor_trzy_pierwsze_litery = tab_kw[min];
        tab_kw[min] = tab_kw[i];
        tab_kw[i] = Bufor_trzy_pierwsze_litery;
    }
    
    cout << "\nTablica po sortowaniu rosnaco:\n";
    for(int i = 0; i < N; i++)
        cout << tab_kw[i] << " ";
    cout << endl;

    return 0;
}
